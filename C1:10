print ("Athulya Siji 21MCA014")
rows=int(input("enter number of rows"))
cols=int(input("enter number of coloums"))
print("enter the elements of first matrix:")
matrix_a= [[int(input()) for i in range(cols)] for i in range(rows)]
print("first matrix is:")
for n in matrix_a:
    print(n)
print("enter the elements of second matrix:")
matrix_b = [[int(input()) for i in range(cols)] for i in range(rows)]
print("second matrix is:")
for n in matrix_b:
    print(n)
result=[[0 for i in range(cols)] for i in range(rows)]
for i in range(rows):
    for j in range(cols):
        result[i][j] = matrix_a[i][j]+matrix_b[i][j]
print("The Sum of Above two Matrices is : ")
for r in result:
    print(r)
